{
  "name": "dragalia-site-front",
  "description": "Frontend of DL info website by OM.",
  "repository": "https://github.com/RaenonX-DL/dragalia-site-front",
  "version": "2.0.0",
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "start": "pm2-runtime start pm2.yml --env production",
    "build": "npm run build:react && npm run build:service",
    "build:react": "react-scripts build",
    "build:service": "tsc --p ./service/tsconfig.json",
    "test": "react-scripts test",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:md": "remark .",
    "lint:fix": "eslint src --fix --ext .ts,.tsx"
  },
  "dependencies": {
    "@ctrl/react-adsense": "^1.3.1",
    "@reduxjs/toolkit": "^1.5.1",
    "bootstrap": "^4.6.0",
    "chalk": "^4.1.0",
    "color": "^3.1.3",
    "dotenv": "^8.2.0",
    "fastify": "^3.15.1",
    "node-fetch": "^2.6.1",
    "mime": "^2.5.2",
    "pm2": "^4.5.6",
    "react": "^17.0.2",
    "react-beforeunload": "^2.5.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.4",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.1",
    "react-scripts": "^4.0.3",
    "react-timeago": "^5.2.0",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^1.0.0",
    "typescript": "^4.2.4",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/color": "^3.0.1",
    "@types/jest": "^24.9.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.44",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^16.14.5",
    "@types/react-beforeunload": "^2.1.0",
    "@types/react-dom": "^16.9.12",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-hash-link": "^2.0.0",
    "@types/react-timeago": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-unused-imports": "^1.1.1",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
